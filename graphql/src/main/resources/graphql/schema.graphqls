type Query {
    bookById(id: ID): Book
    personByName(name: String): Name
}

type Name {
    id: ID
    name: String
    sex: String
    age: Int
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}